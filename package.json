{
  "name": "express-api-es6-starter",
  "version": "1.0.0",
  "description": "Express API ES6 Starter",
  "scripts": {
    "start": "node dist",
    "precommit": "lint-staged",
    "prestart": "npm run build",
    "lint": "eslint src test; exit 0",
    "lint:fix": "eslint src test --fix; exit 0",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "seed": "knex seed:run --knexfile src/knexfile.js",
    "build": "rimraf dist && babel src -D --ignore src/migrations,src/seeds --out-dir dist",
    "migrate": "knex migrate:latest --knexfile src/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile src/knexfile.js",
    "make:seeder": "knex seed:make --knexfile src/knexfile.js",
    "make:migration": "knex migrate:make --knexfile src/knexfile.js",
    "test": "NODE_ENV=test mocha --recursive --require babel-register test",
    "prettify": "prettier --single-quote --print-width 120 --write \"src/**/*.js\" \"test/**/*.js\"",
    "start:dev": "nodemon -L --watch .env --watch src --exec babel-node src --source-maps",
    "test:coverage": "NODE_ENV=test nyc --require babel-register --reporter lcov --reporter text mocha --recursive test"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6",
    "api",
    "mysql",
    "redis",
    "swagger"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.3",
    "bookshelf-camelcase": "^2.0.1",
    "boom": "^5.1.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "http-status-codes": "^1.1.6",
    "joi": "^11.0.1",
    "jsonwebtoken": "^8.1.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.10",
    "moment": "^2.20.1",
    "morgan": "^1.8.2",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redis": "^2.8.0",
    "serve-favicon": "^2.4.3",
    "swagger-jsdoc": "^1.9.4",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "codecov": "^3.0.4",
    "eslint": "^4.7.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  }
}
